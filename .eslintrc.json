{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
      "linebreak-style": "off",
      "indent": "off",
      "comma-dangle": "off",
      "no-use-before-define": "off",
      "spaced-comment": "off",
      "no-console": "off",
	  "no-undef": "off",
	  "prefer-template": "off",
	  "no-restricted-syntax": "off",
	  "max-len": "off",
	  "no-plusplus": ["error", { "allowForLoopAfterthoughts": true }],
	  "curly": "off",
	  "object-shorthand": "off",
	  "no-alert": "off"
    }
}
